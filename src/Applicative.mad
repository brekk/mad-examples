import type { Maybe } from "Maybe"

import { liftA2 } from "Applicative"
import IO from "IO"
import Integer from "Integer"
import { Just, Nothing } from "Maybe"
import Number from "Number"



main = () => {
  pipe(
    ap(Just((a) => a + 30)),
    IO.trace("OH YEAH"),
  )(Just(100))

  h1 = Just(11)
  h2 = Just(120)
  h3 = Just(1200)

  pipe(
    map((a, b) => a + b),
    ap($, h3),
    IO.trace("a + b"),
  )(h1)

  pipe(
    liftA2((a, b) => a + b, h2),
    IO.trace("LIFT A2 (h1 + h2)"),
  )(h1)

  pipe(
    liftA2((a, b) => a + b, h2),
    IO.trace("LIFT A2 (Nothing + h2)"),
  )(Nothing)
}
